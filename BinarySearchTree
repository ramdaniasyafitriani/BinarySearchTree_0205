#include <iostream>
using namespace std;

class Node
{
public :
    int info;
    Node *leftchild;
    Node *rightchild;

    // Constructor for the node class
    Node()
    {
        leftchild = nullptr; // Initialized left child to null
        rightchild = nullptr; // Initialized right child to null
    }
};

class BinaryTree
{
public :
    Node *ROOT;
    BinaryTree()
    {

    }

};

